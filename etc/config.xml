<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <tooso>
            <active>
                <frontend>1</frontend>
            </active>
            <server>
                <api_key />
                <api_base_url>https://v3dev.api.tooso.ai/</api_base_url>
                <api_version>3</api_version>
                <send_report>1</send_report>
                <debug_mode>0</debug_mode>
            </server>
            <search>
                <fallback_enable>0</fallback_enable>
                <default_limit>250</default_limit>
                <message_style><![CDATA[
    .message.tooso{
        margin: 0 0 10px;
        padding: 12px 20px 12px 25px;
        padding-left: 25px;
        display: block;
        font-size: 1.3rem;
        background: #f0f0f0;
        padding-left: 45px;
        position: relative;
    }
                ]]></message_style>
                <filter_price_override>price</filter_price_override>
            </search>
            <analytics>
                <track_userid>1</track_userid>
            </analytics>
        </tooso>
    </default>
</config>
