<?xml version="1.0"?>
<page layout="3columns" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <referenceContainer name="after.body.start">
        <block class="Bitbull\Tooso\Block\Tracking\Library" cacheable="false" name="tooso_tracking_library" template="Bitbull_Tooso::tracking/library.phtml" />
    </referenceContainer>
    <referenceContainer name="before.body.end">
        <block class="Bitbull\Tooso\Block\Tracking\LibraryInit" cacheable="false" name="tooso_tracking_library_init" template="Bitbull_Tooso::tracking/library_init.phtml" />
    </referenceContainer>
</page>
